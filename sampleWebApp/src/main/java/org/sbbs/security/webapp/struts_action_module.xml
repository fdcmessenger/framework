<struts>
	<package name="module" extends="default" namespace="/security">
		<action name="module_tree_list">
			<result name="success">/WEB-INF/pages/module/module_tree_list.jsp
			</result>

		</action>

		<action name="moduleList"
			class="org.sbbs.security.webapp.action.ModulePagedAction">
			<result name="success">/WEB-INF/pages/module/moduleList.jsp</result>
		</action>
		<action name="moduleGrid"
			class="org.sbbs.security.webapp.action.ModulePagedAction" method="gridPageList">
			<result type="json">
				<param name="includeProperties">pageList.*,total,records,page,statusCode,message</param>
				<param name="noCache">true</param>
				<param name="ignoreHierarchy">false</param>
			</result>
		</action>
		<action name="moduleTree"
			class="org.sbbs.security.webapp.action.ModulePagedAction">
			<result name="success">/WEB-INF/pages/module/moduleTree.jsp
			</result>
		</action>
		<action name="moduleTreeJson"
			class="org.sbbs.security.webapp.action.ModulePagedAction"
			method="displayFullTree">
			<result name="success" type="json">
				<param name="includeProperties">fullTreeList.*</param>
				<param name="excludeProperties">fullTreeList.*\.parentNode,fullTreeList.*\.url</param>
				<param name="noCache">true</param>
				<param name="ignoreHierarchy">false</param>
			</result>
		</action>
			<action name="rebuildModuleTree" class="org.sbbs.security.webapp.action.ModuleMaintainAction" method="rebuildTree">
