<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="organization" extends="default" namespace="/security">
	
	<action name="organization_tree_list">
			<result name="success">/WEB-INF/pages/organization/organization_tree_list.jsp
			</result>

		</action>
	
		<action name="organizationList" class="org.sbbs.security.webapp.action.OrganizationPagedAction">
			<result name="success">/WEB-INF/pages/organization/organizationList.jsp
			</result>
		</action>
		<action name="organizationGrid" class="org.sbbs.security.webapp.action.OrganizationPagedAction" method="gridPageList">
			<result type="json">
				<param name="includeProperties">pageList.*,total,records,page,statusCode,message</param>
				<param name="noCache">true</param>
				<param name="ignoreHierarchy">false</param>
			</result>
		</action>


		<action name="organizationTree" class="org.sbbs.security.webapp.action.OrganizationPagedAction">
			<result name="success">/WEB-INF/pages/organization/organizationTree.jsp
			</result>
		</action>
		<action name="organizationTreeJson" class="org.sbbs.security.webapp.action.OrganizationPagedAction" method="displayFullTree">
			<result name="success" type="json">
				<param name="includeProperties">fullTreeList.*</param>
				<param name="excludeProperties">fullTreeList.*\.parentNode</param>
				<param name="noCache">true</param>
				<param name="ignoreHierarchy">false</param>
			</result>
		</action>
		<action name="rebuildOrgTree" class="org.sbbs.security.webapp.action.ModuleMaintainAction" method="rebuildTree">
		</action>
