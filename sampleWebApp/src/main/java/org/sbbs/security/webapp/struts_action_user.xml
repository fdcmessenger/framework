<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="user" extends="default" namespace="/security">
		<action name="userList" class="org.sbbs.security.webapp.action.UserPagedAction">
			<result name="success">/WEB-INF/pages/user/userList.jsp</result>
		</action>
		<action name="userGrid" class="org.sbbs.security.webapp.action.UserPagedAction" method="gridPageList">
			<result type="json">
				<param name="includeProperties">pageList.*,total,records,page,statusCode,message</param>
				<param name="excludeProperties">pageList.*\.userRoles,pageList.*\.organization.organizationRoles,pageList.*\.organization.users,pageList.*\.organization.parentNode</param>
				<param name="noCache">true</param>
				<param name="ignoreHierarchy">false</param>
			</result>
		</action>

		<action name="updatePwd">
			<result name="success">/WEB-INF/pages/user/updatePwd.jsp</result>
		</action>

		<action name="savePwd" class="org.sbbs.security.webapp.action.UserMaintainAction" method="updatePassword" />


		<action name="editUser" class="org.sbbs.security.webapp.action.UserMaintainAction" method="edit">
			<result name="success">/WEB-INF/pages/user/userForm.jsp</result>
		</action>
		<action name="addUser" class="org.sbbs.security.webapp.action.UserMaintainAction" method="add">
			<result name="success">/WEB-INF/pages/user/userForm.jsp</result>
		</action>
		<action name="saveUser" class="org.sbbs.security.webapp.action.UserMaintainAction" method="save" />
		<action name="deleteUser" class="org.sbbs.security.webapp.action.UserMaintainAction" method="delete" />
		<action name="deleteUsers" class="org.sbbs.security.webapp.action.UserMaintainAction" method="deletes" />
		<action name="resetPsw" class="org.sbbs.security.webapp.action.UserMaintainAction" method="resetPsw" />
		<action name="listUnassignUserRole" class="org.sbbs.security.webapp.action.UserMaintainAction" method="listUnassignRole">
			<result name="success">/WEB-INF/pages/user/assign_user_role.jsp</result>
		</action>
		<action name="assignUserRole" class="org.sbbs.security.webapp.action.UserMaintainAction" method="assignRole" />
		<action name="listAssignedUserRole" class="org.sbbs.security.webapp.action.UserMaintainAction" method="listAssginedUserRole">
			<result name="success">/WEB-INF/pages/user/delete_user_role.jsp</result>
		</action>
		<action name="deleteUserRole" class="org.sbbs.security.webapp.action.UserMaintainAction" method="deleteUserRole" />


		<!-- <action name="exportEntity" class="org.sbbs.demo.webapp.action.DemoDwzPagedAction" method="excelExport"/> <action 
			name="beforeExportEntity" class="org.sbbs.demo.webapp.action.DemoDwzPagedAction" method="beforeExcelExport"/> -->

		


	</package>
</struts>
